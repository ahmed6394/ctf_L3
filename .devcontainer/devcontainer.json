{
    "name": "Pwnbox Dev",
    "image": "pwnbox",
    "workspaceFolder": "/work",
    "mounts": [
        "source=${localWorkspaceFolder},target=/work,type=bind"
    ],
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            }
        }
    },
    "remoteUser": "root"
}
